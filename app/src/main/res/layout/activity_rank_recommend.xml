                    android:background="@drawable/focusd"
                    android:gravity="left"
                    android:onClick="onClick"
                    android:textSize="15sp"
                    android:textColor="#000000"
                    android:fontFamily="@font/font"
                    android:id="@+id/bt1"
                    android:layout_marginBottom="10dp">
                </Button>
